{
  "deepseek": {
    "api_key": "",
    "model": "deepseek-vl-7b-chat",
    "fallback_model": "deepseek-chat",
    "max_tokens": 2000,
    "temperature": 0.1
  },
  "claude": {
    "api_key": "${ANTHROPIC_API_KEY}",
    "model": "claude-3-sonnet-20240229",
    "max_tokens": 2000,
    "temperature": 0.1
  },
  "openai": {
    "api_key": "",
    "model": "gpt-4-vision-preview",
    "max_tokens": 2000,
    "temperature": 0.1
  },
  "gemini": {
    "api_key": "",
    "model": "gemini-pro-vision"
  },
  "paths": {
