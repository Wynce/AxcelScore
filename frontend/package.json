{
  "name": "axcelscore-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run copy-question-banks && vite build",
    "build:symlink": "vite build",
    "copy-question-banks": "cp -r ../question_banks public/ || xcopy ..\\question_banks public\\question_banks\\ /E /I",
    "clean-question-banks": "rm -rf public/question_banks",
    "setup-symlink": "cd public && ln -sf ../../question_banks question_banks && echo 'Symlink created successfully'",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "vite": "^5.4.10",
    "@tailwindcss/postcss": "^4.1.12"
  }
}